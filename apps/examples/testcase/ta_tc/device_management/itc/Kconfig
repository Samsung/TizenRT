  bool "Get Server Addrress API"
  default n
  
config ITC_DM_GET_SERVER_PORT
  bool "Get Server Port API"
  default n

config ITC_DM_GET_CLIENT_LIFETIME
  bool "Get Client lifetime API"
  default n

