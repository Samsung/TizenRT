  bool "Get Client state API"
  default n

config ITC_DM_DISPLAY_CLIENT_RESOURCE
  bool "Display Client Resource API"
  default n

config ITC_DM_CONN_GET_RSSI
  bool "Get Rssi API"
  default n
  
